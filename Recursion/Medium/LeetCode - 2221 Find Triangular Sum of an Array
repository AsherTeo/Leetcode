class Solution:
    def triangularSum(self, nums: List[int]) -> int:
        if len(nums) == 1:
            return nums[0]

        def recursion(nums):

            if len(nums) == 1:
                return nums[0]
                
            new_num = []
            for i in range(1, len(nums)):
                new_num.append((nums[i - 1] + nums[i]) % 10)

            return recursion(new_num)

        return recursion(nums)
