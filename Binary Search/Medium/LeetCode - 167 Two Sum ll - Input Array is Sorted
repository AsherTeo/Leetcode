class Solution(object):
    def twoSum(self, numbers, target):

        for i in range(len(numbers)):
            l = i + 1
            r = len(numbers) - 1
            diff = target - numbers[i]

            while l <= r:
                mid = (l + r)//2
                if numbers[mid] == diff:
                    return [i + 1, mid + 1]
                elif numbers[mid] < diff:
                    l = mid + 1
                else:
                    r = mid - 1
