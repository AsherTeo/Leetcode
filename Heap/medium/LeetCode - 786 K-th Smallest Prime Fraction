class Solution:
    def kthSmallestPrimeFraction(self, arr: List[int], k: int) -> List[int]:
        heap = []
        for i in range(len(arr)):
            for j in range(i + 1, len(arr)):
                heapq.heappush(heap, (arr[i]/arr[j], arr[i] , arr[j]))

        count = 0
        
        while count < k:
            value, i , j = heapq.heappop(heap)
            count += 1
        return [i, j] 
