class Solution:
    def combine(self, n: int, k: int) -> List[List[int]]:
        res = []
        temp = []

        def backtracking(i):
            if len(temp) == k:
                res.append(temp[:])
                return
            
            for j in range(i, n + 1):
                temp.append(j)
                backtracking(j + 1)
                temp.pop()
        
        backtracking(1)
        return res
