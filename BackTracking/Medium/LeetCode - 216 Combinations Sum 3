class Solution:
    def combinationSum3(self, k: int, n: int) -> List[List[int]]:
        res = []
        temp = []

        def backtracking(i, cur_sum):
            if len(temp) == k and cur_sum == n:
                res.append(temp[:])
                return
            
            for j in range(i, 10):
                temp.append(j)
                backtracking(j + 1, cur_sum + j)
                temp.pop()

        backtracking(1, 0)
        return res
