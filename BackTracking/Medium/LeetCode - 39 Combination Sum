class Solution:
    def combinationSum(self, candidates: List[int], target: int) -> List[List[int]]:
        res = []
        temp = []

        def backtracking(i, cur_sum):
            if cur_sum == target:
                res.append(temp[:])
                return

            for j in range(i, len(candidates)):
                if candidates[j] > target or cur_sum > target:
                    continue

                temp.append(candidates[j])
                backtracking(j, cur_sum + candidates[j])
                temp.pop()

        backtracking(0, 0)
        return res
