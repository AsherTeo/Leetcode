class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        ans = []
        hash_map = defaultdict(list)

        for word in strs:
            sorted_word = sorted(word)
            hash_map[tuple(sorted_word)].append(word)
        
        for values in hash_map.values():
            ans.append(values)
        return ans
