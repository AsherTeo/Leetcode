class Solution:
    def countGoodStrings(self, low: int, high: int, zero: int, one: int) -> int:
        mod = 10**9 + 7
        memo = {}

        def dfs(length):

            if length in memo:
                return memo[length]

            if length > high:
                return 0

            total = 0

            if low <= length <= high:
                total = 1

            total += dfs(length + zero)
            total += dfs(length + one)

            memo[length] = total % mod
            
            return memo[length] 
        
        return dfs(0)
