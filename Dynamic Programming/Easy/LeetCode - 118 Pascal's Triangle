class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        
        if numRows == 1:
            return [[1]]

        if numRows == 2:
            return [[1], [1,1]]

        res = [[1], [1,1]]

        while numRows > 2: 
            cur = []
            prev = res[-1]

            for i in range(1, len(prev)):
                cur.append(prev[i] + prev[i - 1])
            res.append([1] + cur + [1])
            numRows -= 1
        return res
