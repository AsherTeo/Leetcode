class Solution:
    def getRow(self, rowIndex: int) -> List[int]:
        
        if rowIndex == 0:
            return [1]

        if rowIndex == 1:
            return [1,1]

        prev = [1,1]

        while rowIndex > 1:
            res = []
            for i in range(1, len(prev)):
                res.append(prev[i] + prev[i - 1])
            res = [1] + res + [1]
            prev = res
            rowIndex -= 1
            
        if rowIndex == 1:
            return res
